{
    "build.boot|project.clj": {
        "resources/*": {
            "type": "resource"
        },
        "src/*.clj": {
            "alternate": "test/{}_test.clj",
            "type": "src"
        },
        "test/*_test.clj": {
            "alternate": "src/{}.clj",
            "type": "test"
        }
    }
}
